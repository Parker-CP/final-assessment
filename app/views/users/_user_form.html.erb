<%= form_for @user do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email, class: "form-control" %>
  <%= f.label :password %>
  <%= f.password_field :password, class: "form-control" %>
  <%= f.password_confirmation :password_confirmation, class: "form-control" %>
  <%= f.submit class: "btn btn-primary col-md-offset-4t" %>
<% end %>
